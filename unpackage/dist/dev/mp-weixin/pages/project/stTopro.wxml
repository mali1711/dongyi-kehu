<view class="content"><swiper indicator-dots="true" autoplay="true"><swiper-item><image src="../../static/projectSwiper.png" mode="aspectFit"></image></swiper-item></swiper><view class="projectList"><block wx:for="{{projectList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="projecttitle"><navigator url="{{'/pages/project/projectDetails?pr_id='+item.pr_id+'&pic_1='+pic_1+'&subscribetime='+subscribetime+'&stname='+stname+'&st_id='+st_id}}"><vrow class="row" vue-id="{{'66ff4698-1-'+index}}" bind:__l="__l" vue-slots="{{['default']}}"><vcol vue-id="{{('66ff4698-2-'+index)+','+('66ff4698-1-'+index)}}" span="30" bind:__l="__l" vue-slots="{{['default']}}"><view class="col"><image class="projectImg" src="{{item.photo}}" mode="aspectFill"></image></view></vcol><vcol class="desc" vue-id="{{('66ff4698-3-'+index)+','+('66ff4698-1-'+index)}}" span="70" bind:__l="__l" vue-slots="{{['default']}}"><view class="_h5">{{item.title}}</view><text><view class="_b">简介:</view>{{item.desc}}</text><text class="price"><view class="_b">价格：</view><label class="_span">{{item.price+"/次"}}</label></text></vcol></vrow></navigator></view></block></view></view>