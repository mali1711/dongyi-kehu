<view class="content"><form data-event-opts="{{[['submit',[['formSubmit',['$event']]]],['reset',[['formReset',['$event']]]]]}}" bindsubmit="__e" bindreset="__e"><input class="address" hidden="true" name="longitude" type="text" value="{{longitude}}"/><input class="address" hidden="true" name="latitude" type="text" value="{{latitude}}"/><uni-list vue-id="1" bind:__l="__l" vue-slots="{{['default']}}"><uni-list-item vue-id="{{('2')+','+('1')}}" title="服务地址" bind:__l="__l" vue-slots="{{['default']}}"><input class="address" name="address" type="text" value="{{address}}"/></uni-list-item><uni-list-item vue-id="{{('3')+','+('1')}}" title="姓名" bind:__l="__l" vue-slots="{{['default']}}"><input class="name" name="name" type="text" value="{{name}}"/></uni-list-item><uni-list-item vue-id="{{('4')+','+('1')}}" title="联系电话" show-badge="{{true}}" bind:__l="__l" vue-slots="{{['default']}}"><input class="mobile" name="mobile" type="text" value="{{mobile}}"/></uni-list-item></uni-list><view class="logout"><button class="submit" form-type="submit" type="warn">保存</button></view></form></view>